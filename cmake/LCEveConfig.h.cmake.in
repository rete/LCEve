#pragma once

/* Version information automatically generated by installer. */

/*
 * These macros can be used in the following way:
 *
 *    #if LCEVE_VERSION_CODE >= LCEVE_VERSION(2,23,4)
 *       #include <newheader.h>
 *    #else
 *       #include <oldheader.h>
 *    #endif
 *
*/

#define LCEVE_RELEASE "@LCEve_VERSION_MAJOR@.@LCEve_VERSION_MINOR@.@LCEve_VERSION_PATCH@"
#define LCEVE_RELEASE_DATE "@LCEve_RELEASE_DATE@"
#define LCEVE_RELEASE_TIME "@LCEve_RELEASE_TIME@"
#define LCEVE_VERSION(a,b,c) (((a) << 16) + ((b) << 8) + (c))
#define LCEVE_VERSION_CODE LCEVE_VERSION(@LCEve_VERSION_MAJOR@,@LCEve_VERSION_MINOR@,@LCEve_VERSION_PATCH@)
#define LCEVE_DIR "@CMAKE_INSTALL_PREFIX@"
